Issue: #311
---
 src/js/FormModel.js | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/src/js/FormModel.js b/src/js/FormModel.js
index af3dcc9..d078e4c 100644
--- a/src/js/FormModel.js
+++ b/src/js/FormModel.js
@@ -1188,5 +1188,9 @@ define( function( require, exports, module ) {
         }
     };
 
+    // Expose types to facilitate extending with custom types
+    // TODO: refactor
+    FormModel.prototype.types = types;
+
     module.exports = FormModel;
 } );
